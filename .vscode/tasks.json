{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "go",
			"label": "go: build package",
			"command": "build",
			"args": [
				"-o",
				"${env:HOME}/go/bin/${workspaceFolderBasename}",
				"${workspaceFolder}"
			],
			"problemMatcher": [
				"$go"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		}
	]
}